  "babel": {
    "presets": [
      "latest",
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "standard",
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
    },
    "plugins": [
      "react"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-anfema",
    "rules": {
      "indentation": 2
    }
  },
  "scripts": {
    "eslint": "eslint src test tools postcss.config.js webpack.config.js",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --compilers js:babel-register",
    "test:watch": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --compilers js:babel-register --reporter min --watch",
    "build": "node tools/build.js",
    "build:debug": "node tools/build.js --debug",
    "publish": "node tools/publish.js",
    "publish:debug": "node tools/publish.js --debug",
    "start": "node tools/run.js"
  }
}
